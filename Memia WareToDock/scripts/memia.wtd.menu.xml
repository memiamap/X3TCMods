<line linenr="095" indent=""><var>$DockSector&#160;=&#160;</var><var>$Choice&#160;-&gt;</var><text>&#160;get&#160;sector</text></line>
<line linenr="096" indent=""><var>$Stations&#160;=&#160;</var><var>$DockSector&#160;-&gt;</var><text>&#160;get&#160;player&#160;owned&#160;station&#160;array&#160;from&#160;sector</text></line>
