<line linenr="026" indent="&#160;&#160;"><var>$Stations&#160;=&#160;</var><var>$Sector&#160;-&gt;</var><text>&#160;get&#160;player&#160;owned&#160;station&#160;array&#160;from&#160;sector</text></line>
<line linenr="027" indent="&#160;&#160;"><var>$CountA&#160;=&#160;</var><text>&#160;size&#160;of&#160;array&#160;</text><var>$Stations</var></line>
<line linenr="028" indent="&#160;&#160;"><var>while&#160;</var><var>$CountA</var><text>&#160;</text><var>&gt;</var><text>&#160;</text><var>0</var></line>
<line linenr="029" indent="&#160;&#160;&#160;"><text>dec&#160;</text><var>$CountA&#160;=&#160;</var></line>
<line linenr="030" indent="&#160;&#160;&#160;"><var>$Station&#160;=&#160;</var><var>$Stations</var><text>[</text><var>$CountA</var><text>]</text></line>
<line linenr="031" indent="&#160;&#160;&#160;"></line>
<line linenr="032" indent="&#160;&#160;&#160;"><var>if&#160;</var><var>$Station&#160;-&gt;</var><text>&#160;is&#160;of&#160;class&#160;</text><var>Dock</var></line>
<line linenr="033" indent=""><comment><text>*&#160;</text><var>Add&#160;docks</var></comment></line>
<line linenr="034" indent="&#160;&#160;&#160;&#160;"><var>$Name&#160;=&#160;</var><var>$Station&#160;-&gt;</var><text>&#160;get&#160;name</text></line>
<line linenr="035" indent="&#160;&#160;&#160;&#160;"><var>if&#160;</var><var>$Station&#160;-&gt;</var><text>&#160;get&#160;local&#160;variable:&#160;name=</text><var>$IsWtdEnabledVar</var></line>
<line linenr="036" indent="&#160;&#160;&#160;&#160;&#160;"><var>$Title&#160;=&#160;</var><text>sprintf:&#160;pageid=</text><var>$PageID</var><text>&#160;textid=</text><var>9000</var><text>,&#160;</text><var>$Name</var><text>,&#160;</text><var>null</var><text>,&#160;</text><var>null</var><text>,&#160;</text><var>null</var><text>,&#160;</text><var>null</var></line>
<line linenr="037" indent="&#160;&#160;&#160;&#160;"><text>else</text></line>
<line linenr="038" indent="&#160;&#160;&#160;&#160;&#160;"><var>$Title&#160;=&#160;</var><var>$Name</var></line>
<line linenr="039" indent="&#160;&#160;&#160;&#160;"><text>end</text></line>
<line linenr="040" indent="&#160;&#160;&#160;&#160;"><text>add&#160;custom&#160;menu&#160;item&#160;to&#160;array&#160;</text><var>$MainMenu</var><text>:&#160;text=</text><var>$Title</var><text>&#160;returnvalue=</text><var>$Station</var></line>
<line linenr="041" indent="&#160;&#160;&#160;"><var>else&#160;if&#160;</var><var>$Station&#160;-&gt;</var><text>&#160;is&#160;of&#160;class&#160;</text><var>Station</var></line>
<line linenr="042" indent=""><comment><text>*&#160;</text><var>Add&#160;station&#160;to&#160;array&#160;for&#160;processing&#160;later</var></comment></line>
<line linenr="043" indent="&#160;&#160;&#160;&#160;"><text>append&#160;</text><var>$Station</var><text>&#160;to&#160;array&#160;</text><var>$SectorStations</var></line>
<line linenr="044" indent="&#160;&#160;&#160;"><text>end</text></line>
<line linenr="045" indent="&#160;&#160;"><text>end</text></line>
<line linenr="046" indent="&#160;&#160;"></line>
<line linenr="047" indent=""><comment><text>*&#160;</text><var>Process&#160;stations</var></comment></line>
<line linenr="048" indent="&#160;&#160;"><text>add&#160;section&#160;to&#160;custom&#160;menu:&#160;</text><var>$MainMenu</var></line>
<line linenr="049" indent="&#160;&#160;"><var>$SectorStations&#160;=&#160;</var><text>&#160;sort&#160;array&#160;</text><var>$SectorStations</var></line>
<line linenr="050" indent="&#160;&#160;"><var>$SectorStations&#160;=&#160;</var><text>&#160;reverse&#160;array&#160;</text><var>$SectorStations</var></line>
<line linenr="051" indent="&#160;&#160;"><var>$CountA&#160;=&#160;</var><text>&#160;size&#160;of&#160;array&#160;</text><var>$SectorStations</var></line>
<line linenr="052" indent="&#160;&#160;"><var>while&#160;</var><var>$CountA</var><text>&#160;</text><var>&gt;</var><text>&#160;</text><var>0</var></line>
<line linenr="053" indent="&#160;&#160;&#160;"><text>dec&#160;</text><var>$CountA&#160;=&#160;</var></line>
<line linenr="054" indent="&#160;&#160;&#160;"><var>$Station&#160;=&#160;</var><var>$SectorStations</var><text>[</text><var>$CountA</var><text>]</text></line>
<line linenr="055" indent="&#160;&#160;&#160;"></line>
<line linenr="056" indent=""><comment><text>*&#160;</text><var>Change&#160;display&#160;depending&#160;on&#160;whether&#160;WTD&#160;is&#160;enabled</var></comment></line>
<line linenr="057" indent="&#160;&#160;&#160;"><var>$Name&#160;=&#160;</var><var>$Station&#160;-&gt;</var><text>&#160;get&#160;name</text></line>
<line linenr="058" indent="&#160;&#160;&#160;"><var>if&#160;</var><var>$Station&#160;-&gt;</var><text>&#160;get&#160;local&#160;variable:&#160;name=</text><var>$IsWtdEnabledVar</var></line>
<line linenr="059" indent="&#160;&#160;&#160;&#160;"><var>$Title&#160;=&#160;</var><text>sprintf:&#160;pageid=</text><var>$PageID</var><text>&#160;textid=</text><var>9000</var><text>,&#160;</text><var>$Name</var><text>,&#160;</text><var>null</var><text>,&#160;</text><var>null</var><text>,&#160;</text><var>null</var><text>,&#160;</text><var>null</var></line>
<line linenr="060" indent="&#160;&#160;&#160;"><text>else</text></line>
<line linenr="061" indent="&#160;&#160;&#160;&#160;"><var>$Title&#160;=&#160;</var><var>$Name</var></line>
<line linenr="062" indent="&#160;&#160;&#160;"><text>end</text></line>
<line linenr="063" indent="&#160;&#160;&#160;"><text>add&#160;custom&#160;menu&#160;item&#160;to&#160;array&#160;</text><var>$MainMenu</var><text>:&#160;text=</text><var>$Title</var><text>&#160;returnvalue=</text><var>$Station</var></line>
<line linenr="064" indent="&#160;&#160;"><text>end</text></line>
<line linenr="065" indent="&#160;"><text>end</text></line>
<line linenr="066" indent="&#160;"></line>
<line linenr="067" indent=""><comment><text>*&#160;</text><var>Open&#160;the&#160;menu&#160;and&#160;process&#160;result</var></comment></line>
<line linenr="068" indent="&#160;"><var>$Choice&#160;=&#160;</var><text>&#160;open&#160;custom&#160;menu:&#160;title=</text><var>$MenuTitle</var><text>&#160;description=</text><var>$MenuDesc</var><text>&#160;option&#160;array=</text><var>$MainMenu</var></line>
<line linenr="069" indent="&#160;"><var>if&#160;</var><var>$Choice</var><text>&#160;</text><var>==</var><text>&#160;</text><var>-1</var></line>
<line linenr="070" indent="&#160;&#160;"><text>goto&#160;label&#160;</text><var>Exit</var></line>
<line linenr="071" indent="&#160;"><text>else</text></line>
<line linenr="072" indent=""><comment><text>*&#160;</text><var>Is&#160;WTD&#160;already&#160;running&#160;on&#160;the&#160;object?</var></comment></line>
<line linenr="073" indent="&#160;&#160;"><var>$Status&#160;=&#160;</var><var>$Choice&#160;-&gt;</var><text>&#160;get&#160;local&#160;variable:&#160;name=</text><var>$IsWtdEnabledVar</var></line>
<line linenr="074" indent="&#160;&#160;"><var>if&#160;</var><var>$Status</var></line>
